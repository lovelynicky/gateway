<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.mvc.repository.McMemberRepository">
    <select id="getMcMemberByUid" parameterType="java.util.Map" resultType="com.spring.mvc.model.McMemberModel">
        select * from ims_mc_members where uid=#{uid};
    </select>
</mapper>