<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.mvc.repository.AddressRepository">
    <select id="getAddressById" parameterType="int" resultType="com.spring.mvc.model.AddressModel">
        select * from ims_eso_sale_address where id=#{id};
    </select>
</mapper>